{
  "name": "brm-commercial-frontend",
  "version": "1.0.0",
  "description": "Gestão Comercial - Módulo Relação Comercial",
  "scripts": {
    "start": "webpack-dev-server --config webpack.config.local.babel.js --open --port 3000 --hot --inline --progress --colors --watch",
    "start:dev": "webpack-dev-server --config webpack.config.dev.babel.js --open --port 3000 --hot --inline --progress --colors --watch",
    "lint": "node_modules/.bin/esw webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --compilers js:babel-core/register --recursive --compilers js:babel-core/register --recursive --reporter progress test/setup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "build": "webpack --config webpack.config.prod.babel.js --progress --colors --bail --display-error-details",
    "build:dev": "webpack --config webpack.config.dev.babel.js --progress --colors --bail --display-error-details",
    "build:hml": "webpack --config webpack.config.hml.babel.js --progress --colors --bail --display-error-details",
    "mock": "json-server ./mock/db.json --port 9000",
    "build:langs:en": "cross-env node scripts/translate.js",
    "build:langs:es": "cross-env LOCALE=\"es\" node scripts/translate.js",
    "build:langs:ptbr": "cross-env LOCALE=\"pt-BR\" node scripts/translate.js",
    "build:langs": "npm-run-all build:langs:en build:langs:es build:langs:ptbr"
  },
  "author": "Accenture",
  "license": "Natura",
